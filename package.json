{
  "name": "custom-javaScript-for-websites-2",
  "version": "3.4.5",
  "description": "Run custom JavaScript on any website.",
  "scripts": {
    "analyze": "NODE_ENV=production node utils/analyze.js",
    "build": "node utils/build.js",
    "deploy": "NODE_ENV=production yarn build && yarn zip",
    "zip": "zip -X --compression-method deflate -r build.zip build",
    "prettier": "prettier --no-semi --single-quote --write",
    "lint:ts": "standard --parser typescript-eslint-parser --plugin typescript ./**/*.tsx",
    "lint": "standard",
    "lintfix": "yarn prettier 'src/**/*.{js,ts,tsx}' && yarn lint --fix && yarn lint:ts --fix",
    "start": "node utils/webserver.js",
    "upload:chrome": "node -r dotenv/config ./node_modules/.bin/webstore upload --source build.zip",
    "publish:chrome": "node -r dotenv/config ./node_modules/.bin/webstore publish",
    "publish-extension": "yarn publish:chrome",
    "release": "standard-version",
    "test": "yarn lint && yarn build"
  },
  "dependencies": {
    "@material-ui/core": "4.9.14",
    "@material-ui/styles": "4.9.14",
    "antd": "4.2.4",
    "chrome-extension-async": "3.4.1",
    "clean-webpack-plugin": "3.0.0",
    "js-beautify": "1.11.0",
    "lodash.orderby": "4.6.0",
    "lodash.uniqby": "4.7.0",
    "mobx": "5.15.4",
    "mobx-react": "6.2.2",
    "object-sizeof": "1.6.0",
    "query-string": "6.12.1",
    "react": "16.13.1",
    "react-ace": "8.1.0",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.112",
    "@types/jasmine": "3.5.10",
    "@types/jest": "25.2.3",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-import": "1.13.0",
    "chrome-webstore-upload-cli": "1.2.0",
    "codecov": "3.7.0",
    "copy-webpack-plugin": "6.0.1",
    "coveralls": "3.1.0",
    "css-loader": "3.5.3",
    "dotenv": "8.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "7.0.0",
    "eslint-plugin-typescript": "0.14.0",
    "file-loader": "6.0.0",
    "fork-ts-checker-webpack-plugin": "4.1.4",
    "hard-source-webpack-plugin": "0.13.1",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.4",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "prettier": "2.0.5",
    "progress-bar-webpack-plugin": "2.1.0",
    "react-axe": "3.4.1",
    "react-test-renderer": "16.13.1",
    "regenerator-runtime": "0.13.5",
    "samsam": "1.3.0",
    "sinon": "9.0.2",
    "sinon-chrome": "3.0.1",
    "speed-measure-webpack-plugin": "1.3.3",
    "standard": "14.3.4",
    "standard-version": "8.0.0",
    "style-loader": "1.2.1",
    "terser-webpack-plugin": "3.0.7",
    "ts-jest": "26.0.0",
    "ts-loader": "7.0.4",
    "typescript": "3.9.2",
    "typescript-eslint-parser": "22.0.0",
    "web-ext": "4.2.0",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.7.0",
    "webpack-dev-server": "3.11.0",
    "write-file-webpack-plugin": "4.5.1"
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "standard --fix"
    ],
    "*.{ts,tsx}": [
      "yarn prettier",
      "standard --parser typescript-eslint-parser --plugin typescript --fix"
    ],
    "*.{json,css,md}": [
      "yarn prettier"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "src/js/chrome-extension-async.js",
      "src/lib"
    ],
    "globals": [
      "ace",
      "chrome"
    ]
  }
}
