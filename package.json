{
  "name": "custom-javaScript-for-websites-2",
  "version": "3.4.9",
  "description": "Run custom JavaScript on any website.",
  "scripts": {
    "analyze": "NODE_ENV=production node utils/analyze.js",
    "build": "NODE_ENV=production node utils/build.js",
    "deploy": "run-s build zip",
    "zip": "zip -X --compression-method deflate -r build.zip build",
    "prettier": "prettier --no-semi --single-quote --write",
    "lint:ts": "standard --parser @typescript-eslint/parser --plugin typescript ./**/*.tsx",
    "lint": "standard",
    "lintfix": "yarn prettier 'src/**/*.{js,ts,tsx}' && yarn lint --fix && yarn lint:ts --fix",
    "start": "node utils/webserver.js",
    "upload:chrome": "node -r dotenv/config ./node_modules/.bin/webstore upload --source build.zip",
    "publish:chrome": "node -r dotenv/config ./node_modules/.bin/webstore publish",
    "publish-extension": "yarn publish:chrome",
    "release": "standard-version",
    "test": "run-s lint build"
  },
  "dependencies": {
    "@material-ui/core": "4.11.3",
    "@material-ui/styles": "4.11.3",
    "antd": "4.13.1",
    "chrome-extension-async": "3.4.1",
    "clean-webpack-plugin": "3.0.0",
    "js-beautify": "1.13.5",
    "lodash.orderby": "4.6.0",
    "lodash.uniqby": "4.7.0",
    "mobx": "6.1.8",
    "mobx-react": "7.1.0",
    "object-sizeof": "1.6.1",
    "query-string": "6.14.1",
    "react": "17.0.1",
    "react-ace": "9.3.0",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0"
  },
  "devDependencies": {
    "@types/chrome": "0.0.133",
    "@types/jasmine": "3.6.7",
    "@types/jest": "26.0.21",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.2",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-import": "1.13.3",
    "chrome-webstore-upload-cli": "1.2.1",
    "codecov": "3.8.1",
    "copy-webpack-plugin": "7.0.0",
    "coveralls": "3.1.0",
    "css-loader": "5.1.3",
    "dotenv": "8.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.22.0",
    "eslint-plugin-typescript": "0.14.0",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "6.2.0",
    "hard-source-webpack-plugin": "0.13.1",
    "html-loader": "1.3.2",
    "html-webpack-plugin": "4.5.2",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "mini-css-extract-plugin": "1.3.9",
    "npm-run-all": "4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "prettier": "2.2.1",
    "progress-bar-webpack-plugin": "2.1.0",
    "react-axe": "3.5.4",
    "react-test-renderer": "17.0.1",
    "regenerator-runtime": "0.13.7",
    "samsam": "1.3.0",
    "sinon": "9.2.4",
    "sinon-chrome": "3.0.1",
    "standard": "16.0.3",
    "standard-version": "9.1.1",
    "style-loader": "2.0.0",
    "terser-webpack-plugin": "5.1.1",
    "ts-jest": "26.5.4",
    "ts-loader": "8.0.18",
    "typescript": "4.2.3",
    "web-ext": "5.5.0",
    "webpack": "5.27.2",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-dev-server": "3.11.2",
    "write-file-webpack-plugin": "4.5.1"
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "standard --fix"
    ],
    "*.{ts,tsx}": [
      "yarn prettier",
      "standard --parser @typescript-eslint/parser --plugin typescript --fix"
    ],
    "*.{json,css,md}": [
      "yarn prettier"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ],
    "parser": "babel-eslint",
    "ignore": [
      "src/js/chrome-extension-async.js",
      "src/lib"
    ],
    "globals": [
      "screen",
      "Event",
      "HTMLElement",
      "HTMLInputElement",
      "HTMLDivElement",
      "page",
      "browser",
      "ace",
      "chrome"
    ]
  }
}
